<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RosaMolas\facturacionBundle\Entity\Factura" table="factura">
    <indexes>
      <index name="factura_periodo_escolar_alumnos_id" columns="periodo_escolar_alumnos_id"/>
      <index name="factura_tipo_factura_id" columns="tipo_factura_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="factura_nombre_key" columns="nombre"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="monto" type="decimal" column="monto" precision="8" scale="2" nullable="false"/>
    <field name="activo" type="boolean" column="activo" nullable="false"/>
    <many-to-one field="tipoFactura" target-entity="RosaMolas\facturacionBundle\Entity\TipoFactura">
      <join-columns>
        <join-column name="tipo_factura_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="periodoEscolarAlumnos" target-entity="PeriodoEscolarAlumno">
      <join-columns>
        <join-column name="periodo_escolar_alumnos_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
