<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RosaMolas\usuariosBundle\Entity\ParentescoAlumnosRepresentantes" table="parentesco_alumnos_representantes">
    <indexes>
      <index name="IDX_53FB6EA62FD20D28" columns="representante_id"/>
      <index name="IDX_53FB6EA6FC28E5EE" columns="alumno_id"/>
      <index name="IDX_53FB6EA65BA311FC" columns="parentesco_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="parentesco" target-entity="RosaMolas\genericoBundle\Entity\Parentescos">
      <join-columns>
        <join-column name="parentesco_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="alumno" target-entity="RosaMolas\alumnosBundle\Entity\Alumnos">
      <join-columns>
        <join-column name="alumno_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="representante" target-entity="RosaMolas\usuariosBundle\Entity\Usuarios">
      <join-columns>
        <join-column name="representante_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
