<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RosaMolas\genericoBundle\Entity\InscripcionAlumnoRepresentanteDatos" table="inscripcion_alumno_representante_datos">
    <indexes>
      <index name="inscripcion_IDX_FE46CA10FC28E5EE" columns="alumno_id"/>
      <index name="inscripcion_IDX_FE46CA102FD20D28" columns="representante_id"/>
      <index name="inscripcion_IDX_FE46CA105BA311FC" columns="parentesco_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="alumno_representante_unico" columns="alumno_id,representante_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="principal" type="boolean" column="principal" nullable="false"/>
    <many-to-one field="parentesco" target-entity="RosaMolas\genericoBundle\Entity\Parentescos">
      <join-columns>
        <join-column name="parentesco_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="representante" target-entity="RosaMolas\genericoBundle\Entity\InscripcionUsuarios">
      <join-columns>
        <join-column name="representante_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="alumno" target-entity="RosaMolas\genericoBundle\Entity\InscripcionAlumnos" inversed-by="alumnoRepresentanteDatos">
      <join-columns>
        <join-column name="alumno_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
