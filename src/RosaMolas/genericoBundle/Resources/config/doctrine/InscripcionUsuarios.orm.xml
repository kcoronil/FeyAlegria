<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="RosaMolas\genericoBundle\Entity\InscripcionUsuarios" table="inscripcion_usuarios">
        <indexes>
            <index name="inscripcion_usuarios_sexo_id" columns="sexo_id"/>
            <index name="inscripcion_IDX_EF687F24ABE41B6" columns="tipo_usuario_id"/>
        </indexes>
        <unique-constraints>
            <unique-constraint name="inscripcion_usuarios_cedula_key" columns="cedula"/>
        </unique-constraints>
        <id name="id" type="integer" column="id">
            <generator strategy="SEQUENCE"/>
        </id>
        <field name="cedula" type="integer" column="cedula" nullable="false"/>
        <field name="primerApellido" type="string" column="primer_apellido" length="30" nullable="false"/>
        <field name="primerNombre" type="string" column="primer_nombre" length="30" nullable="false"/>
        <field name="segundoApellido" type="string" column="segundo_apellido" length="30" nullable="true"/>
        <field name="segundoNombre" type="string" column="segundo_nombre" length="30" nullable="true"/>
        <field name="fechaNacimiento" type="date" column="fecha_nacimiento" nullable="false"/>
        <field name="direccion" type="text" column="direccion" nullable="false"/>
        <field name="email" type="string" column="email" length="256" nullable="true"/>
        <field name="principal" type="boolean" column="principal" nullable="false"/>
        <field name="activo" type="boolean" column="activo" nullable="false"/>
        <one-to-many field="inscripcion" mapped-by="representante" target-entity="Inscripcion">
            <cascade>
                <cascade-persist/>
            </cascade>
        </one-to-many>
        <one-to-many field="representanteContacto" mapped-by="representante" target-entity="InscripcionRepresentanteContacto">
            <cascade>
                <cascade-persist/>
            </cascade>
        </one-to-many>
        <many-to-one field="sexo" target-entity="RosaMolas\genericoBundle\Entity\Sexo">
            <join-columns>
                <join-column name="sexo_id" referenced-column-name="id"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="tipoUsuario" target-entity="RosaMolas\usuariosBundle\Entity\TipoUsuario">
            <join-columns>
                <join-column name="tipo_usuario_id" referenced-column-name="id"/>
            </join-columns>
        </many-to-one>
        <many-to-many field="alumno" target-entity="RosaMolas\genericoBundle\Entity\InscripcionAlumnos" mapped-by="representante">
            <cascade>
                <cascade-persist/>
            </cascade>
            <join-table name="inscripcion_alumnos_representantes">
                <join-columns>
                    <join-column name="representante_id" referenced-column-name="id"/>
                </join-columns>
                <inverse-join-columns>
                    <join-column name="alumno_id" referenced-column-name="id"/>
                </inverse-join-columns>
            </join-table>
        </many-to-many>
    </entity>
</doctrine-mapping>

